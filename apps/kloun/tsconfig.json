{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": [
      "vite/client"
    ],
    "typeRoots": [
      "./node_modules/@types",
      "./src/types"
    ]
  },
  "compileOnSave": false,
  "exclude": [
    "src",
    "wwwroot"
  ],
  "include": [
    "required.ts"
  ]
}